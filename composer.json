{
    "name": "tattali/utm-cookie-bundle",
    "type": "symfony-bundle",
    "description": "This bundle allow you to save utm parameters from url into a cookie when they exists. It's also provide a bunch of tools to easily retrive all or each utm.",
    "homepage": "https://github.com/tattali/UtmCookieBundle",
    "keywords": ["utm", "cookie", "symfony", "symfony utm"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yannick Cenatiempo",
            "email": "yannick.cenatiempo@medelse.com"
        },
        {
            "name": "ThÃ©o Attali",
            "email": "theoattali@gmail.com",
            "homepage": "https://theoattali.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^3.4 || ^4.0 || ^5.0 || ^6.0",
        "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "UtmCookieBundle\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-nette": "^0.12",
        "phpstan/phpstan": "^0.12",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
